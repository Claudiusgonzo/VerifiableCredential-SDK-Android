{
  "name": "@microsoft/useragent-sdk",
  "version": "1.0.0-alpha.1",
  "description": "SDK for building decentralized identity wallets and enterprise agents.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/UserAgent.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/UserAgent/issues"
  },
  "keywords": [
    "Decentralized Identity",
    "DID",
    "UserAgent"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "main": "dist/src/index.js",
  "scripts": {
    "precommit": "npm run lint && npm run build:docs && git add docs/sdk.md",
    "build": "tsc --p tsconfig.json",
    "test": "nyc jasmine-ts --config=./tests/jasmine.json",
    "lint": "tslint --fix --project . --config ./tslint.json",
    "lint-pr": "tslint --project . --config ./tslint.json",
    "build:docs": "jsdoc2md --files src/**/*.ts --configure jsdoc2md.json > docs/sdk.md"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-typescript": "7.3.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jasmine": "3.3.12",
    "@types/lodash": "4.14.123",
    "@types/memdown": "3.0.0",
    "@types/node": "11.13.8",
    "@types/pouchdb": "6.3.3",
    "@types/pouchdb-adapter-memory": "6.1.3",
    "fetch-mock": "7.3.3",
    "husky": "2.2.0",
    "jasmine": "3.4.0",
    "jasmine-reporters": "2.3.2",
    "jasmine-spec-reporter": "4.2.1",
    "jasmine-ts": "0.3.0",
    "jsdoc-babel": "0.5.0",
    "jsdoc-to-markdown": "4.0.1",
    "nyc": "14.1.0",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-config-standard": "8.0.1",
    "tslint-microsoft-contrib": "6.1.1",
    "tsconfig-paths": "3.8.0"
  },
  "dependencies": {
    "@decentralized-identity/did-auth-jose": "0.1.12",
    "@decentralized-identity/did-common-typescript": "0.1.18",
    "@decentralized-identity/did-crypto-typescript": "0.1.6",
    "@decentralized-identity/hub-node-core": "0.1.4",
    "@decentralized-identity/hub-sdk-js": "0.1.3",
    "base64url": "3.0.1",
    "crypto-pouch": "3.1.3",
    "es6-promise": "4.2.6",
    "hub-mongo-connector": "0.1.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.11",
    "multihashes": "0.4.14",
    "node-webcrypto-ossl": "^1.0.44",
    "pouchdb": "7.0.0",
    "pouchdb-adapter-memory": "7.0.0",
    "typescript": "3.4.5"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html"
    ]
  }
}
